{
    "cluster": "ecs-ec2",
    "serviceName": "scaling-replication",
    "taskDefinition": "<task_def>",
    "loadBalancers": [
        {
            "targetGroupArn": "arn:aws:elasticloadbalancing:us-east-1:997477333591:targetgroup/ecs-ec2-tg2/1cd6aed8180765ac",
            "containerName": "flaskapp",
            "containerPort": 80
        }
    ],
    "desiredCount": 2,
    "launchType": "EC2",
    "schedulingStrategy": "REPLICA",
    "healthCheckGracePeriodSeconds": 330,
    "deploymentController": {
        "type": "CODE_DEPLOY"
    },
    "tags": [
        {
            "key": "auto-delete",
            "value": "never"
        }
    ],
    "enableECSManagedTags": true,
    "propagateTags": "TASK_DEFINITION"
}
